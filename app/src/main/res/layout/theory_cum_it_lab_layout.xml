                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoInternalSignage"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 12. CCTV Cameras with Audio Facility -->
            <TextView
                android:id="@+id/labelCCTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cctv_cameras_with_audio_facility"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/cctv"
                app:layout_constraintTop_toBottomOf="@id/valueInternalSignage"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoCCTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelCCTV"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueCCTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoCCTV"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 13. LAN Enabled Computers (In No.) -->
            <TextView
                android:id="@+id/labelLANComputers"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lan_enabled_computers_in_no"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/lan"
                app:layout_constraintTop_toBottomOf="@id/valueCCTV"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoLANComputers"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelLANComputers"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueLANComputers"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoLANComputers"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 14. Internet Connections -->
            <TextView
                android:id="@+id/labelInternet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/internet_connections"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/internet"
                app:layout_constraintTop_toBottomOf="@id/valueLANComputers"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoInternet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelInternet"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueInternet"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoInternet"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 15. Do all Computers have Typing Tutor -->
            <TextView
                android:id="@+id/labelTypingTutor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/do_all_computers_have_typing_tutor"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/tuition"
                app:layout_constraintTop_toBottomOf="@id/valueInternet"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoTypingTutor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
