            <TextView
                android:id="@+id/valueTypingTutor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoTypingTutor"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 16. Tablets (In No.) -->
            <TextView
                android:id="@+id/labelTablets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tablets_in_no"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/tablet"
                app:layout_constraintTop_toBottomOf="@id/valueTypingTutor"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoTablets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelTablets"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueTablets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoTablets"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 17. Stools/Chairs (In No.) -->
            <TextView
                android:id="@+id/labelStools"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/stools_chairs_in_no"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/stool"
                app:layout_constraintTop_toBottomOf="@id/valueTablets"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoStools"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelStools"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueStools"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoStools"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 18. Trainer Chair -->
            <TextView
                android:id="@+id/labelTrainerChair"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/trainer_chair"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/chair"
                app:layout_constraintTop_toBottomOf="@id/valueStools"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoTrainerChair"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelTrainerChair"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueTrainerChair"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoTrainerChair"
