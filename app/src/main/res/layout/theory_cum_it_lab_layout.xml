<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/scroll"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:background="@color/color_light_grey">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/dp_100">

        <!-- Toolbar -->
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_100"
            android:background="@color/color_dark_blue"
            android:elevation="4dp"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/backButton"
                android:layout_marginStart="@dimen/dp_20"
                android:padding="@dimen/dp_5"
                android:scaleType="fitCenter"
                android:src="@drawable/back_arrow"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/theory_cum_it_lab"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_16"/>

            <com.google.android.material.imageview.ShapeableImageView
                android:layout_width="150dp"
                android:layout_height="@dimen/dp_60"
                android:layout_gravity="end"
                android:background="@color/white"
                android:padding="@dimen/dp_5"
                android:scaleType="fitCenter"
                android:src="@drawable/ddy_logo"
                app:shapeAppearanceOverlay="@style/CustomRectangleShape" />



        </androidx.appcompat.widget.Toolbar>


        <!-- Section Start -->
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/trainingInfraFields"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            app:layout_constraintTop_toBottomOf="@id/toolbar"
            android:layout_margin="@dimen/dp_20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <!-- 1. Type of Roof -->
            <TextView
                android:id="@+id/labelTypeOfRoof"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/type_of_roof"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_roof"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/yesNoTypeOfRoof"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelTypeOfRoof"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/valueTypeOfRoof"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoTypeOfRoof"
                app:layout_constraintStart_toStartOf="parent"/>

            <!-- 2. False Ceiling Provided -->
            <TextView
                android:id="@+id/labelFalseCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/false_ceiling_provided"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_walls"
                app:layout_constraintTop_toBottomOf="@id/valueTypeOfRoof"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/yesNoFalseCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelFalseCeiling"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/valueFalseCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoFalseCeiling"
                app:layout_constraintStart_toStartOf="parent"/>

            <!-- 3. Height of Ceiling -->
            <TextView
                android:id="@+id/labelHeightCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/height_of_ceiling_in_ft"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_special"
                app:layout_constraintTop_toBottomOf="@id/valueFalseCeiling"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/yesNoHeightCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelHeightCeiling"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/valueHeightCeiling"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoHeightCeiling"
                app:layout_constraintStart_toStartOf="parent"/>

            <!-- 4. Ventilation Area (In Sq. ft) -->
            <TextView
                android:id="@+id/labelVentilationArea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ventilation_area_in_sq_ft"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_square_feet"
                app:layout_constraintTop_toBottomOf="@id/valueHeightCeiling"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoVentilationArea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelVentilationArea"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueVentilationArea"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoVentilationArea"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 5. Sound Level (In dB) -->
            <TextView
                android:id="@+id/labelSoundLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sound_level_in_db"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_sound"
                app:layout_constraintTop_toBottomOf="@id/valueVentilationArea"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoSoundLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelSoundLevel"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueSoundLevel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoSoundLevel"
                app:layout_constraintStart_toStartOf="parent" />

            <!-- 6. Are Academic Centres Sound Proofed & Air Conditioned -->
            <TextView
                android:id="@+id/labelSoundProofAC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/whether_all_the_academic_centres_have_been_sound_proofed_with_air_conditioning"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ac"
                app:layout_constraintTop_toBottomOf="@id/valueSoundLevel"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoSoundProofAC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelSoundProofAC"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueSoundProofAC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/view"
                android:textColor="@color/color_dark_blue"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/yesNoSoundProofAC"
                app:layout_constraintStart_toStartOf="parent" />
            <!-- 10. Academic Room Information Board -->
            <TextView
                android:id="@+id/labelInfoBoard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/academic_room_information_board"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/ic_special"
                app:layout_constraintTop_toBottomOf="@id/valueSoundProofAC"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoInfoBoard"
