            <!-- 20. Lights (In No.) -->
            <TextView
                android:id="@+id/labelLights"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/lights_in_no"
                android:textColor="@color/color_dark_green"
                android:textSize="@dimen/sp_18"
                android:layout_marginTop="16dp"
                android:drawablePadding="8dp"
                app:drawableStartCompat="@drawable/light"
                app:layout_constraintTop_toBottomOf="@id/valueTrainerTable"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/yesNoLights"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/n_a"
                android:layout_marginStart="@dimen/dp_35"
                app:layout_constraintTop_toBottomOf="@id/labelLights"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/valueLights"
